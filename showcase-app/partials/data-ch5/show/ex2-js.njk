(function(){
    document.getElementById('change-to-ch5-show-3').addEventListener('click', () => {
        document.getElementsByClassName('custom-dom-changes').forEach((i, e) => {
            e.setAttribute('data-ch5-show', 'data_ch5_show_2_2');
        });
    });

    document.getElementById('remove-ch5-show').addEventListener('click', () => {
        document.getElementsByClassName('custom-dom-changes').forEach((i, e) => {
            e.removeAttribute('data-ch5-show');
        });
    });

    document.getElementById('add-ch5-show').addEventListener('click', () => {
        document.getElementsByClassName('custom-dom-changes').forEach((i, e) => {
            e.setAttribute('data-ch5-show', 'data_ch5_show_2_1');
        });
    });

    document.getElementById('rem-ch5-show-el').addEventListener('click', () => {
        document.getElementsByClassName('custom-dom-changes').forEach((i, e) => {
            e.remove();
        });
    });
})(j);
