        "copyfiles": "^2.4.1", 
        "webpack-dev-server": "3.1.4", 
        "@types/hammerjs": "^2.0.40", 
        "dotenv-webpack": "^1.7.0", 
        "wrapper-webpack-plugin": "^2.2.2", 
        "webpack": "^4.35.0", 
        "@types/chai": "^4.2.21", 
        "webpack-cli": "^4.10.0", 
        "@types/semver": "^7.3.8", 
        "typescript": "^4.8.3", 
        "@types/i18next": "^13.0.0", 
        "extract-text-webpack-plugin": "^4.0.0-beta.0", 
        "@types/lodash.throttle": "^4.1.6", 
        "url-loader": "^4.1.1", 
        "@types/mocha": "7.0.2", 
        "@types/dom-inputevent": "^1.0.7", 
        "bower": "^1.8.12", 
        "rimraf": "^3.0.2", 
        "local-web-server": "4", 
        "jsdom": "^16.7.0", 
        "webpack-merge": "^4.2.1", 
        "tslint-loader": "3.6.0", 
        "cross-env": "^7.0.3", 
        "ts-loader": "4.3.0", 
        "tslint": "^6.1.3", 
        "autoprefixer": "^10.3.1", 
        "npm-run-all": "^4.1.5", 
        "tslint-config-prettier": "^1.18.0", 
        "chalk": "^4.1.2", 
        "jsdom-global": "3.0.2", 
        "@types/node": "^16.4.11", 
        "webpack-concat-files-plugin": "0.5.2", 
        "@types/swiper": "^5.4.3"
    }, 
    "resolutions": {
        "**/event-stream": "^4.0.1"
    }, 
    "types": "build_bundles/umd/@types/index.d.ts", 
    "name": "@crestron/ch5-crcomlib"
